 "version": "0.2.0",
 "configurations": [
  {
   "name": "glance",
   "request": "launch",
   "type": "dart",
   "toolArgs": [
    "--dart-define",
    "USE_FIREBASE_EMU=true",
   ]
  },
  {
   "name": "glance (real device)",
   "request": "launch",
   "type": "dart",
   "toolArgs": [
    "--dart-define",
    "USE_FIREBASE_EMU=true",
    "--dart-define",
    "FIREBASE_EMU_URL=$(ipconfig getifaddr en0)",
   ]
  },
  {
   "name": "glance (profile mode)",
   "request": "launch",
   "type": "dart",
   "flutterMode": "profile",
   "toolArgs": [
    "--dart-define",
    "USE_FIREBASE_EMU=false",
   ]
  },
  {
   "name": "glance (release mode)",
   "request": "launch",
   "type": "dart",
   "flutterMode": "release",
   "toolArgs": [
    "--dart-define",
    "USE_FIREBASE_EMU=false",
   ]
  }
 ]
}